# configuration

.PHONY: all clean install uninstall

BUILD = './build'

# .PHONY targets

all:
	$(MAKE) -C $(BUILD) all

clean:
	$(MAKE) -C $(BUILD) clean

install:
	$(MAKE) -C $(BUILD) install

uninstall:
	$(MAKE) -C $(BUILD) uninstall

